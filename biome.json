{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"files": {
		"maxSize": 9128312873
	},
	"vcs": {
		"clientKind": "git",
		"enabled": false,
		"root": "./"
	},
	"linter": {
		"enabled": false
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "tab",
		"indentWidth": 4,
		"lineWidth": 120,
		"lineEnding": "lf"
	},
	"organizeImports": {
		"enabled": true
	},
	"javascript": {
		"formatter": {
			"enabled": true,
			"semicolons": "always",
			"trailingComma": "es5",
			"quoteProperties": "asNeeded",
			"quoteStyle": "double",
			"bracketSpacing": true
		}
	}
}
